{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/indentifirer.js"
  ],
  "exclude": [
    "app/tab/backend/**",
    "app-example/**",
    "node_modules/**"
  ]
}
